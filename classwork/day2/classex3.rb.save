class Student
attr_accessor :name, :department, :cgpa
def initialize(name, dept, cgpa)
@name=name
@department=dept
@cgpa=cgpa
end
def has_oe
if @cgpa >= 7.5
return true
end
return false
end
end
student1=Student.new("AKhil", "Civl", 8.2)
if student1.has_oe == true
puts "Elgible for OE"
end
s "Not elgible for OE"
end
